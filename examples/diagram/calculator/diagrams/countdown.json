{
  "$schema": "https://raw.githubusercontent.com/open-rmf/crossflow/refs/heads/main/diagram.schema.json",
  "version": "0.1.0",
  "templates": {},
  "start": "comparison",
  "ops": {
    "comparison": {
      "type": "node",
      "builder": "less_than",
      "next": "fork",
      "config": 1,
      "display_text": "is Less Than 1"
    },
    "fork": {
      "type": "fork_result",
      "err": "parallel",
      "ok": {
        "builtin": "terminate"
      }
    },
    "subtract": {
      "type": "node",
      "builder": "sub",
      "next": "comparison",
      "config": 1
    },
    "print": {
      "type": "node",
      "builder": "print",
      "next": {
        "builtin": "dispose"
      },
      "config": "Subtracting one",
      "display_text": "Print Subtraction"
    },
    "parallel": {
      "type": "fork_clone",
      "next": [
        "print",
        "subtract"
      ]
    }
  }
}