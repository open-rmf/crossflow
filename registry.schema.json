{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "DiagramElementMetadata",
  "type": "object",
  "properties": {
    "messages": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/MessageMetadata"
      }
    },
    "nodes": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/$defs/NodeMetadata"
      }
    },
    "schemas": {
      "type": "object",
      "additionalProperties": true
    },
    "sections": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/$defs/SectionMetadata"
      }
    },
    "trace_supported": {
      "type": "boolean"
    }
  },
  "required": [
    "nodes",
    "sections",
    "messages",
    "schemas",
    "trace_supported"
  ],
  "$defs": {
    "ConfigExample": {
      "type": "object",
      "properties": {
        "description": {
          "description": "A description of what this config is for",
          "type": "string"
        },
        "config": {
          "description": "The value of the config"
        }
      },
      "required": [
        "description",
        "config"
      ]
    },
    "MessageMetadata": {
      "type": "object",
      "properties": {
        "operations": {
          "$ref": "#/$defs/MessageOperationMetadata"
        },
        "schema": {
          "anyOf": [
            {
              "$ref": "#/$defs/Schema"
            },
            {
              "type": "null"
            }
          ]
        },
        "type_name": {
          "type": "string"
        }
      },
      "required": [
        "type_name",
        "operations"
      ]
    },
    "MessageOperationMetadata": {
      "type": "object",
      "properties": {
        "deserialize": {
          "type": [
            "object",
            "null"
          ]
        },
        "fork_clone": {
          "type": [
            "object",
            "null"
          ]
        },
        "fork_result": {
          "type": [
            "object",
            "null"
          ]
        },
        "join": {
          "type": [
            "object",
            "null"
          ]
        },
        "serialize": {
          "type": [
            "object",
            "null"
          ]
        },
        "split": {
          "type": [
            "object",
            "null"
          ]
        },
        "unzip": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": [
              "integer",
              "null"
            ],
            "format": "uint",
            "minimum": 0
          }
        }
      }
    },
    "NodeMetadata": {
      "type": "object",
      "properties": {
        "description": {
          "type": [
            "string",
            "null"
          ]
        },
        "config_examples": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/ConfigExample"
          }
        },
        "config_schema": {
          "$ref": "#/$defs/Schema"
        },
        "default_display_text": {
          "description": "If the user does not specify a default display text, the node ID will\n be used here.",
          "type": "string"
        },
        "request": {
          "type": "integer",
          "format": "uint",
          "minimum": 0
        },
        "response": {
          "type": "integer",
          "format": "uint",
          "minimum": 0
        },
        "streams": {
          "type": "object",
          "additionalProperties": {
            "type": "integer",
            "format": "uint",
            "minimum": 0
          }
        }
      },
      "required": [
        "default_display_text",
        "request",
        "response",
        "streams",
        "config_schema",
        "config_examples"
      ]
    },
    "Schema": {
      "type": [
        "object",
        "boolean"
      ]
    },
    "SectionBuffer": {
      "type": "object",
      "properties": {
        "message_type": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint",
          "minimum": 0
        }
      }
    },
    "SectionInput": {
      "type": "object",
      "properties": {
        "message_type": {
          "type": "integer",
          "format": "uint",
          "minimum": 0
        }
      },
      "required": [
        "message_type"
      ]
    },
    "SectionInterface": {
      "type": "object",
      "properties": {
        "buffers": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/$defs/SectionBuffer"
          }
        },
        "inputs": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/$defs/SectionInput"
          }
        },
        "outputs": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/$defs/SectionOutput"
          }
        }
      },
      "required": [
        "inputs",
        "outputs",
        "buffers"
      ]
    },
    "SectionMetadata": {
      "type": "object",
      "properties": {
        "description": {
          "type": [
            "string",
            "null"
          ]
        },
        "config_examples": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/ConfigExample"
          }
        },
        "config_schema": {
          "$ref": "#/$defs/Schema"
        },
        "default_display_text": {
          "type": "string"
        },
        "interface": {
          "$ref": "#/$defs/SectionInterface"
        }
      },
      "required": [
        "default_display_text",
        "interface",
        "config_schema",
        "config_examples"
      ]
    },
    "SectionOutput": {
      "type": "object",
      "properties": {
        "message_type": {
          "type": "integer",
          "format": "uint",
          "minimum": 0
        }
      },
      "required": [
        "message_type"
      ]
    }
  }
}