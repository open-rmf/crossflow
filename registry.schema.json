{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "DiagramElementMetadata",
  "type": "object",
  "properties": {
    "messages": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/MessageMetadata"
      }
    },
    "nodes": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/$defs/NodeMetadata"
      }
    },
    "reverse_message_lookup": {
      "$ref": "#/$defs/ReverseMessageLookup"
    },
    "schemas": {
      "type": "object",
      "additionalProperties": true
    },
    "sections": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/$defs/SectionMetadata"
      }
    },
    "trace_supported": {
      "type": "boolean"
    }
  },
  "required": [
    "nodes",
    "sections",
    "messages",
    "schemas",
    "reverse_message_lookup",
    "trace_supported"
  ],
  "$defs": {
    "BufferMapLayoutHints": {
      "description": "Hints for how a buffer map might be laid out.\n\nThis can be thought of as a schema for the buffer map.",
      "oneOf": [
        {
          "description": "The buffers in this map and their message types are not necessarily known in advance.",
          "type": "object",
          "properties": {
            "dynamic": {
              "$ref": "#/$defs/DynamicBufferMapLayoutHints"
            }
          },
          "additionalProperties": false,
          "required": [
            "dynamic"
          ]
        },
        {
          "description": "The buffer identifiers and their message types are fixed in advance.",
          "type": "object",
          "properties": {
            "static": {
              "type": "object",
              "additionalProperties": {
                "$ref": "#/$defs/MessageTypeHint"
              },
              "patternProperties": {
                "^\\d+$": {
                  "$ref": "#/$defs/MessageTypeHint"
                }
              }
            }
          },
          "additionalProperties": false,
          "required": [
            "static"
          ]
        }
      ]
    },
    "ConfigExample": {
      "type": "object",
      "properties": {
        "config": {
          "description": "The value of the config"
        },
        "description": {
          "description": "A description of what this config is for",
          "type": "string"
        }
      },
      "required": [
        "description",
        "config"
      ]
    },
    "DynamicBufferMapLayoutHints": {
      "type": "object",
      "properties": {
        "hint": {
          "description": "Constraints for the message types of the buffers.",
          "anyOf": [
            {
              "$ref": "#/$defs/MessageTypeHint"
            },
            {
              "type": "null"
            }
          ]
        },
        "indices": {
          "description": "The buffer identifiers can include indices.",
          "type": "boolean"
        },
        "names": {
          "description": "The buffer identifiers can include names.",
          "type": "boolean"
        }
      },
      "required": [
        "indices",
        "names"
      ]
    },
    "MessageMetadata": {
      "type": "object",
      "properties": {
        "operations": {
          "anyOf": [
            {
              "$ref": "#/$defs/MessageOperationsMetadata"
            },
            {
              "type": "null"
            }
          ]
        },
        "schema": {
          "anyOf": [
            {
              "$ref": "#/$defs/Schema"
            },
            {
              "type": "null"
            }
          ]
        },
        "type_name": {
          "type": "string"
        }
      },
      "required": [
        "type_name"
      ]
    },
    "MessageOperationsMetadata": {
      "type": "object",
      "properties": {
        "deserialize": {
          "type": [
            "object",
            "null"
          ]
        },
        "fork_clone": {
          "type": [
            "object",
            "null"
          ]
        },
        "fork_result": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "integer",
            "format": "uint",
            "minimum": 0
          },
          "maxItems": 2,
          "minItems": 2
        },
        "from": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "uint",
            "minimum": 0
          },
          "uniqueItems": true
        },
        "into": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "uint",
            "minimum": 0
          },
          "uniqueItems": true
        },
        "join": {
          "anyOf": [
            {
              "$ref": "#/$defs/BufferMapLayoutHints"
            },
            {
              "type": "null"
            }
          ]
        },
        "serialize": {
          "type": [
            "object",
            "null"
          ]
        },
        "split": {
          "type": [
            "object",
            "null"
          ]
        },
        "try_from": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "uint",
            "minimum": 0
          },
          "uniqueItems": true
        },
        "try_into": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "uint",
            "minimum": 0
          },
          "uniqueItems": true
        },
        "unzip": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "integer",
            "format": "uint",
            "minimum": 0
          }
        }
      },
      "required": [
        "into",
        "try_into",
        "from",
        "try_from"
      ]
    },
    "MessageTypeHint": {
      "description": "This hint is used by the diagram builder to assign types to buffers who do\nnot have any messages pushed into them directly as input.",
      "oneOf": [
        {
          "description": "An accessor is asking specifically for this type T via `BufferKey<T>`",
          "type": "object",
          "properties": {
            "exact": {
              "type": "integer",
              "format": "uint",
              "minimum": 0
            }
          },
          "additionalProperties": false,
          "required": [
            "exact"
          ]
        },
        {
          "description": "An accessor is using a generalized buffer, e.g. JsonBuffer or AnyBuffer,\nwhich can be represented by this type, but an exact type should be used\nif any other accessor has one.",
          "type": "object",
          "properties": {
            "fallback": {
              "type": "integer",
              "format": "uint",
              "minimum": 0
            }
          },
          "additionalProperties": false,
          "required": [
            "fallback"
          ]
        }
      ]
    },
    "NodeMetadata": {
      "type": "object",
      "properties": {
        "config_examples": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/ConfigExample"
          }
        },
        "config_schema": {
          "$ref": "#/$defs/Schema"
        },
        "default_display_text": {
          "description": "If the user does not specify a default display text, the node ID will\nbe used here.",
          "type": "string"
        },
        "description": {
          "type": [
            "string",
            "null"
          ]
        },
        "request": {
          "type": "integer",
          "format": "uint",
          "minimum": 0
        },
        "response": {
          "type": "integer",
          "format": "uint",
          "minimum": 0
        },
        "streams": {
          "type": "object",
          "additionalProperties": {
            "type": "integer",
            "format": "uint",
            "minimum": 0
          }
        }
      },
      "required": [
        "default_display_text",
        "request",
        "response",
        "streams",
        "config_schema",
        "config_examples"
      ]
    },
    "ReverseMessageLookup": {
      "type": "object",
      "properties": {
        "json_message": {
          "description": "The index where the [`JsonMessage`] type is registered.",
          "type": [
            "integer",
            "null"
          ],
          "format": "uint",
          "minimum": 0
        },
        "result": {
          "description": "Map from [T, E] output registrations to Result<T, E> registration.",
          "type": "array",
          "items": {
            "type": "array",
            "maxItems": 2,
            "minItems": 2,
            "prefixItems": [
              {
                "type": "array",
                "items": {
                  "type": "integer",
                  "format": "uint",
                  "minimum": 0
                },
                "maxItems": 2,
                "minItems": 2
              },
              {
                "type": "integer",
                "format": "uint",
                "minimum": 0
              }
            ]
          }
        },
        "split": {
          "description": "Map from the message type of the item that comes out of a split to all\nmessage types that can be split into it.",
          "type": "array",
          "items": {
            "type": "array",
            "maxItems": 2,
            "minItems": 2,
            "prefixItems": [
              {
                "type": "integer",
                "format": "uint",
                "minimum": 0
              },
              {
                "type": "array",
                "items": {
                  "type": "integer",
                  "format": "uint",
                  "minimum": 0
                }
              }
            ]
          }
        },
        "unzip": {
          "description": "Map from the unzipped types to the original zipped type.",
          "type": "array",
          "items": {
            "type": "array",
            "maxItems": 2,
            "minItems": 2,
            "prefixItems": [
              {
                "type": "array",
                "items": {
                  "type": "integer",
                  "format": "uint",
                  "minimum": 0
                }
              },
              {
                "type": "integer",
                "format": "uint",
                "minimum": 0
              }
            ]
          }
        }
      },
      "required": [
        "result",
        "unzip",
        "split"
      ]
    },
    "Schema": {
      "type": [
        "object",
        "boolean"
      ]
    },
    "SectionBuffer": {
      "type": "object",
      "properties": {
        "message_type": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint",
          "minimum": 0
        }
      }
    },
    "SectionInput": {
      "type": "object",
      "properties": {
        "message_type": {
          "type": "integer",
          "format": "uint",
          "minimum": 0
        }
      },
      "required": [
        "message_type"
      ]
    },
    "SectionInterface": {
      "type": "object",
      "properties": {
        "buffers": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/$defs/SectionBuffer"
          }
        },
        "inputs": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/$defs/SectionInput"
          }
        },
        "outputs": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/$defs/SectionOutput"
          }
        }
      },
      "required": [
        "inputs",
        "outputs",
        "buffers"
      ]
    },
    "SectionMetadata": {
      "type": "object",
      "properties": {
        "config_examples": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/ConfigExample"
          }
        },
        "config_schema": {
          "$ref": "#/$defs/Schema"
        },
        "default_display_text": {
          "type": "string"
        },
        "description": {
          "type": [
            "string",
            "null"
          ]
        },
        "interface": {
          "$ref": "#/$defs/SectionInterface"
        }
      },
      "required": [
        "default_display_text",
        "interface",
        "config_schema",
        "config_examples"
      ]
    },
    "SectionOutput": {
      "type": "object",
      "properties": {
        "message_type": {
          "type": "integer",
          "format": "uint",
          "minimum": 0
        }
      },
      "required": [
        "message_type"
      ]
    }
  }
}